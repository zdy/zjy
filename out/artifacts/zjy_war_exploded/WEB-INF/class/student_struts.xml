<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="volunteer" namespace="/" extends="struts-default">
		<default-action-ref name="default" />
		<global-allowed-methods>regex:.*</global-allowed-methods><!-- 适配通配符 struts2.5 -->
		<action name="default">
			<result>/error.jsp</result>
		</action>
		<action name="student_*" class="com.tgzzzxweb.action.StudentAction" method="{1}">
			<result name ="success">/ShowAllStudent.jsp</result>
			<result name="input">/addstudent.jsp</result>
			<result name="selectStudent">/selectshowstudent.jsp</result>
		</action>
	</package>
</struts>